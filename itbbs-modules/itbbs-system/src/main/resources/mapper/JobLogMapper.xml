<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.abc.itbbs.system.mapper.JobLogMapper">

    <resultMap type="JobLog" id="JobLogResult">
            <result property="jobLogId"    column="job_log_id"    />
            <result property="jobName"    column="job_name"    />
            <result property="invokeTarget"    column="invoke_target"    />
            <result property="jobMessage"    column="job_message"    />
            <result property="exceptionInfo"    column="exception_info"    />
            <result property="createTime"    column="create_time"    />
            <result property="updateTime"    column="update_time"    />
            <result property="status"    column="status"    />
            <result property="ver"    column="ver"    />
    </resultMap>

    <sql id="selectJobLogVo">
        select job_log_id, job_name, invoke_target, job_message, exception_info, create_time, update_time, status, ver from tb_job_log
    </sql>

    <select id="selectJobLogList" resultMap="JobLogResult">
        <include refid="selectJobLogVo" />
    </select>


</mapper>
